pipe:
  - type: query
    statement: >-
      SELECT {{#each properties}}"{{ nameSnake }}"{{~#unless @last}},{{/unless}}{{/each}} 
      FROM "{{ nameSnake }}" ;    
    resultType: set
    assign: List{{ name }}
  - type: mapping
    from: List{{ name }}
    isArray: true
    builtIn: snakeToCamel
    to: List{{ name }}
returnVar: List{{ name }}
transactional: false