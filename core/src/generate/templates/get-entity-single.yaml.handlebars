operations:
  - id: GetSingle{{ name }}
    type: sql
    template: >-
      SELECT {{#each properties}}"{{ nameSnake }}"{{~#unless @last}},{{/unless}}{{/each}} 
      FROM "{{nameSnake}}" where "id" = $1;
    params:
      - params.id
    singleRow: true
    assign: 
      var: Single{{ name }}
return: Single{{ name }}