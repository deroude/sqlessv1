operations:
  - id: Update{{ name }}
    type: sql
    template: >-
      UPDATE "{{ nameSnake }}" SET 
      {{#each properties}}{{#unless isId}}"{{ nameSnake }}" = ${{inc @index}} {{~#unless @last}},{{/unless}}{{/unless}}{{/each}}
      WHERE id = $1 RETURNING *
    params:
      - params.id
      {{~#each properties}}
      {{#unless isId}}- body.{{ name }}{{#if fk}}.id{{/if}}{{/unless}}
      {{/each}}
    singleRow: true      
    assign: 
      var: Updated{{ name }}
return: Updated{{ name }}